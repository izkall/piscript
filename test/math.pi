println(floor(3.7))               // Expected: 3
println(floor([3.7, 4.9, 2.1]))  // Expected: [3, 4, 2]

println(ceil(3.2))                // Expected: 4
println(ceil([3.2, 4.1, 2.9]))   // Expected: [4, 5, 3]

println(round(3.5))               // Expected: 4
println(round([3.5, 4.4, 2.6]))  // Expected: [4, 4, 3]

println(sqrt(16))                 // Expected: 4
println(sqrt([4, 9, 16]))        // Expected: [2, 3, 4]

println(sin(0))                  // Expected: 0
println(cos(0))                  // Expected: 1
println(tan(0))                  // Expected: 0

println(min([5, 3, 9, 1, 6]))   // Expected: 1
println(max([5, 3, 9, 1, 6]))   // Expected: 9

println(abs(-5))                 // Expected: 5
println(abs([-5, 2, -3]))       // Expected: [5, 2, 3]

println(rand())                  // Expected: random number between 0 and 1
println(rand_n(5))               // Expected: list of 5 random numbers between 0 and 1
println(rand_i(1, 10))           // Expected: random int between 1 and 10

println(pow(2, 3))               // Expected: 8
println(pow([2, 3, 4], 2))      // Expected: [4, 9, 16]

println(sum([1,2,3,4,5]))       // Expected: 15
println(mean([1,2,3,4,5]))      // Expected: 3
println(avg([1,2,3,4,5]))       // Expected: 3

println(var([1,2,3,4,5]))       // Expected: 2.5
println(dev([1,2,3,4,5]))       // Expected: ~1.58

println(median([1,3,3,6,7,8,9]))// Expected: 6
println(mode([1,2,2,3,3,3,4]))  // Expected: 3
